<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.effect.DropShadow?>

<StackPane fx:id="stackPane" styleClass="menu-cell" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="20.0" />
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label fx:id="price" maxWidth="-Infinity" minWidth="-Infinity" text="Label" GridPane.hgrow="NEVER" />
         </children>
      </GridPane>
      <Label fx:id="name" text="Label" />
   </children>
   <effect>
      <DropShadow height="10.0" radius="4.5" width="10.0" />
   </effect>
</StackPane>
