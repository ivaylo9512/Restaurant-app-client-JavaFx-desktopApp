<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" styleClass="alert-box" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.base.ControllerAlert">
    <children>
      <GridPane fx:id="content" minHeight="150.0" minWidth="200.0" styleClass="content">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints fillHeight="false" maxHeight="-Infinity" percentHeight="33.0" valignment="CENTER" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
          <RowConstraints percentHeight="33.0" vgrow="NEVER" />
        </rowConstraints>
         <children>
            <Label fx:id="alertMessage" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="h" GridPane.rowIndex="1" />
            <Button fx:id="nextButton" styleClass="next-button" maxHeight="1.7976931348623157E308" onMouseClicked="#nextAlert" text="ok" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" GridPane.vgrow="ALWAYS" />
            <Label fx:id="alertsCount" alignment="CENTER" contentDisplay="CENTER" text="Label" />
            <Button fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="x" GridPane.rowIndex="0"  maxWidth="-Infinity" GridPane.halignment="RIGHT"/>
         </children>
      </GridPane>
    </children>
</AnchorPane>
